{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "famatic-api",
  "builds": [
    {
      "src": "api/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
}